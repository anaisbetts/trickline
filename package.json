{
  "name": "trickline",
  "version": "0.0.1",
  "description": "A fast, offline Slack client",
  "main": "src/main.js",
  "scripts": {
    "install": "bower install",
    "start": "cross-env DEBUG=logger:* electron . -r . --enable-logging",
    "test-renderer": "electron-mocha --renderer --require ./test/support.js ./test",
    "test": "npm run test-renderer",
    "package": "electron-packager-compile ./ trickline --platform=win32 --arch=x64 --prune --asar --version 1.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulcbetts/trickline.git"
  },
  "author": "Paul Betts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulcbetts/trickline/issues"
  },
  "homepage": "https://github.com/paulcbetts/trickline#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "dexie-observable": "^0.1.9",
    "electron-compile": "^4.0.0",
    "electron-debug": "^1.0.1",
    "lru-cache": "^4.0.1",
    "rxjs": "^5.0.0-rc.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cross-env": "^3.1.2",
    "csslint": "^1.0.3",
    "electron-compilers": "^4.0.1",
    "electron-debug": "^1.0.1",
    "electron-mocha": "^3.1.1",
    "electron-packager": "^8.1.0",
    "electron-prebuilt-compile": "1.4.3",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-plugin-async-to-sync": "^0.5.0",
    "eslint": "~3.8.0",
    "eslint-plugin-react": "^6.4.1"
  }
}
