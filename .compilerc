{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react"
        ],
        "plugins": ["transform-async-to-generator", "transform-object-rest-spread"],
        "sourceMaps": "inline"
      },
      "text/typescript": {
        "experimentalDecorators": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "target": "es5",
        "sourceMap": false,
        "jsx": "react",
        "types": [
          "node"
        ]
      }
    },
    "test": {
      "application/javascript": {
        "coverage": true,
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react"
        ],
        "plugins": ["transform-async-to-generator", "transform-object-rest-spread"],
        "sourceMaps": "inline"
      },
      "text/typescript": {
        "coverage": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "sourceMap": false,
        "target": "es5",
        "jsx": "react",
        "types": [
          "node"
        ]
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.4.0" } }],
          "react"
        ],
        "plugins": ["transform-async-to-generator", "transform-object-rest-spread"],
        "sourceMaps": "none"
      },
      "text/typescript": {
        "experimentalDecorators": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "sourceMap": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "types": [
          "node"
        ]
      }
    }
  }
}
