{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["es2016-node5", "react"],
        "plugins": ["transform-class-properties", "transform-async-to-generator", "transform-runtime"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["es2016-node5", "react"],
        "plugins": ["transform-class-properties", "transform-async-to-generator", "transform-runtime"],
        "sourceMaps": false
      }
    }
  }
}
